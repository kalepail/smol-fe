---
import Home from "../components/Home.svelte";
import Layout from "../layouts/Layout.astro";

const results = await fetch(import.meta.env.PUBLIC_API_URL).then((res) => {
	if (res.ok) return res.json();
	return [];
});
---

<Layout
	title="Smol"
>
	<Home {results} client:visible={{rootMargin: "200px"}} />
</Layout>
