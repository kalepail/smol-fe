---
interface Props {
  href: string;
  label: string;
}
const { href, label } = Astro.props;
---
<a href={href} class="inline-flex items-center gap-2 text-xs text-[#555] hover:text-[#9ae600] uppercase tracking-widest transition-colors group">
  <span class="group-hover:-translate-x-1 transition-transform">‚Üê</span>
  {label}
</a>
